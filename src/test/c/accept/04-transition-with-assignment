configuration:
    Height : 100;
    Width : 100;
    Frontier : Periodic;
    States : {alive, dead};
    Colors : {1, 2};
Neighborhood : MOORE;
Evolution : CONWAY;

transition:
    a = 12 + (3 / 5);
    for i in {1,2,a} do
        if a < 3 then 
            ->alive 
        end
    end
    ->dead
